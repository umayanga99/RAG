        <script type="text/javascript" nonce="brcmtd2409pThL2s9R8o9Q==">
             var tocServletPath = '/bin/broadcom/techdocs2/TOCServlet';
            $('tocList').hide();
            $.getJSON(tocServletPath,{basePath:'/content/broadcom/techdocs/us/en/vmware-cis/vsphere/vsphere/7-0'},function(data, textStatus, jqXHR) {

                createTOCLoop(data, "accordion");

                $('.tocList').html(tocElement);
                $('.loader').hide();
                $('.tocList').show();

                $('.tocList .collapse').on('shown.bs.collapse', function (e) {

                    if ($(this).is(e.target)){
                        $(this).attr("aria-current", true);
                        $(this).parent('li').addClass('active-acc');
                        $('ul.show').last().find('li.active-acc').addClass('has-border');
                        $(this).parent('li').children('a').last().removeClass('fa-plus').addClass('fa-minus');
                    }
                });

                $('.tocList .collapse').on('hidden.bs.collapse', function (e) {

                    if($(this).is(e.target)){
                        $(this).removeAttr("aria-current");
                        $(this).parent('li').removeClass('active-acc');
                        $(this).parent('li').children('a').last().removeClass('fa-minus').addClass('fa-plus');
                    }
                });

                collapseAndSelectCurrentPageAtTOC();
                leftNavScrollTimer = setInterval(leftNavScroll, 300);
            });

            var tocElement = '';
            function createTOCLoop (thisArray, referParentID) {

                $.each(thisArray, function(index, value) {
                  
                    var htmlItemTitle = value.title.replace(/&/g,'&amp;').replace(/</g,'&lt;').replace(/>/g,'&gt;');
                    var htmlItemLink = value.link;

                    if(value.children.length <= 0) {
                        if(referParentID == "accordion") {
                            tocElement += '<li class="parent"><a class="toc-title col-12" href="'+ htmlItemLink +'"><span>' + htmlItemTitle + '</span></a>';
                        } else {
                            tocElement += '<li><a class="toc-title col-12" href="'+htmlItemLink+'"><span>' + htmlItemTitle + '</span></a>';
                        }
                    }
                    else if(value.children.length > 0) {
                        if(referParentID == "accordion") {
                            tocElement += '<li class="parent"><a class="toc-title col-9 col-lg-8" href="'+htmlItemLink+'">' + htmlItemTitle + '</a><a tabindex="0" aria-label="' + htmlItemTitle + '" role="button" class="col-1 fa fa-plus toc-arrow" aria-label="Click to Expand" data-bs-toggle="collapse" aria-expanded="false" data-bs-target="#toc'+frameid(htmlItemLink+''+htmlItemTitle)+'" href="#toc'+frameid(htmlItemLink+''+htmlItemTitle)+'"></a><ul data-parent="#'+ referParentID +'" class="collapse" id="toc'+frameid(htmlItemLink+''+htmlItemTitle)+'">';
                        } else {
                            tocElement += '<li><a class="toc-title col-9 col-lg-8" href="'+htmlItemLink+'">' + htmlItemTitle + '</a><a tabindex="0" aria-label="' + htmlItemTitle + '" role="button" class="col-1 fa fa-plus toc-arrow" aria-label="Click to Expand" data-bs-toggle="collapse" aria-expanded="false" data-bs-target="#toc'+frameid(htmlItemLink+''+htmlItemTitle)+'" href="#toc'+frameid(htmlItemLink+''+htmlItemTitle)+'"></a><ul data-parent="#'+ referParentID +'" class="collapse" id="toc'+frameid(htmlItemLink+''+htmlItemTitle)+'">';
                        }
                        createTOCLoop(value.children, ""+("toc"+frameid(htmlItemLink+''+htmlItemTitle)));
                        tocElement += '</ul>';
                    }
                    tocElement += '</li>';
                });
            }

            function frameid (id) {
                var strid = id.replace(/[^\w\s]/gi, '').split("_").join("").split(" ").join("");
                return strid;
            }

            var leftNavScrollTimer;

            function leftNavScroll() {
                if($('ul.show').last().find('li.active-acc.has-border').length > 0){
                    document.getElementsByClassName("toc tableofcontentsmod tableofcontents")[0].scrollTop = $('ul.show').last().find('li.active-acc.has-border')[0].offsetTop -100;
                    clearLeftNavScrollTimer();
                }else{
                    if($('ul.tocList').last().find('li.active-acc.has-border').length > 0){
                        document.getElementsByClassName("toc tableofcontentsmod tableofcontents")[0].scrollTop = $('ul.tocList').last().find('li.active-acc.has-border')[0].offsetTop -100;
                        clearLeftNavScrollTimer();
                    }else if($('ul.tocList').last().find('li.active-acc').length > 0){
                        document.getElementsByClassName("toc tableofcontentsmod tableofcontents")[0].scrollTop = $('ul.tocList').last().find('li.active-acc')[0].offsetTop -100;
                        clearLeftNavScrollTimer();
                    }
                }
            }

            function clearLeftNavScrollTimer() {
              clearInterval(leftNavScrollTimer);
            }

            function collapseAndSelectCurrentPageAtTOC(){
                var currentPagePath = window.location.pathname + window.location.hash;
                var currentPagePathSplit = currentPagePath.split(['#']);
                currentPagePath = currentPagePathSplit[0];

                if(currentPagePath.startsWith("/content/broadcom/techdocs")){
                    currentPagePath = currentPagePath.split("/content/broadcom/techdocs")[1];
                }

                $(".toc-title").each(function(index, tocElement) {
                    var thisHref = $(this).attr("href");
                    var thisHrefSplit = thisHref.split(['#']);
                    thisHref = thisHrefSplit[0];



                    $(this).removeClass('active-acc');

                    if(thisHref === currentPagePath) {
                        $(this).attr("aria-current", true);
                        $(this).closest(".parent").addClass("active-acc");
                        $(this).parents(".collapse").collapse('show');
                        //$(this).parents(".collapse").parents("li").addClass("active-acc");
                        $(this).parents("li").addClass("active-acc");
                        $(".active-acc").children(".collapse").each(function(index, child){
                            $(this).collapse('show');
                        });

                        $('li.active-acc').each(function () {
                            if ($(this).children('ul').length > 0) {
                                $(this).removeClass('has-border');
                                $(this).addClass('remove-border');
                            }
                        })

                    }

                });

            }
      	</script>
    </div>

</div>

               </div>
            </div>
         </div>
         <div class="col-12 col-lg-8 offset-lg-1 product-detail px-lg-0">
